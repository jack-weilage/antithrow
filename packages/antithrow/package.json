{
	"name": "antithrow",
	"version": "1.1.0",
	"description": "Rust-style Result types for type-safe error handling without exceptions",
	"license": "MIT",
	"author": "Jack Weilage",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jack-weilage/antithrow.git",
		"directory": "packages/antithrow"
	},
	"keywords": [
		"result",
		"error-handling",
		"typescript",
		"functional"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"clean": "rm -rf dist",
		"lint": "bun run lint:publint && bun run lint:types",
		"lint:publint": "publint",
		"lint:types": "tsc --noEmit"
	}
}
